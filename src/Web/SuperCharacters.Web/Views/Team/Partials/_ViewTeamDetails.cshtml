@model SuperCharactersApp.ViewModels.DTO.TeamViewModels.TeamViewModel

<div class="modal fade" id="detailsTeam-@Model.Id" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Edit the chosen team</h5>
            </div>
            <div class="modal-footer">
                <row>
                    <div class="col-lg-12">
                        <label asp-for="TeamName">@Model.TeamName</label>
                    </div>
                    <div class="col-lg-6">
                        <button type="button" class="btn btn-primary">
                           Back
                        </button>
                    </div>
                    </row>
            </div>
        </div>
    </div>
</div>
